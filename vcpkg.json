{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "cppassert",
    "version-string": "0.1.0",
    "builtin-baseline": "9341313ad2d6954446752673b9a277fa265de86f",
    "dependencies": [],
    "default-features": ["backtrace"],
    "features": {
        "backtrace": {
            "description": "Enable backtrace support using libbacktrace or DbgHelp.lib",
            "dependencies": [
                {
                    "name": "libbacktrace",
                    "version>=": "2021-03-14",
                    "platform": "!windows"
                }
            ]
        },
        "tests": {
            "description": "Build the tests",
            "dependencies": [
                {
                    "name": "gtest",
                    "version>=": "1.11.0"
                }
            ]
        }
    }
}